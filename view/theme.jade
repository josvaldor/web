doctype
html
    head
        title Genocan Genome Analysis
    block head-link
        link(rel='shortcut icon', href='/meritbuilders/img/logo/favicon.ico', type='image/x-icon')
        //link(rel='stylesheet', href='/genocan/css/theme.css')
        meta(charset='UTF-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        meta(name='description', content='')
        meta(name='author', content='')
        link(rel='icon', href='../../favicon.ico')
        title Genocan
        link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css')
    block head-script
        script(type='text/javascript' src='/jquery/js/jquery-2.1.4.min.js')
    block head-meta
        meta(name='viewport' content='user-scalable=no; width=device-width; initial-scale=1; maximum-scale=1;')
    body
      nav.navbar.navbar-toggleable-md.navbar-inverse.fixed-top.bg-inverse
        button.navbar-toggler.navbar-toggler-right.hidden-lg-up(type='button', data-toggle='collapse', data-target='#navbarsExampleDefault', aria-controls='navbarsExampleDefault', aria-expanded='false', aria-label='Toggle navigation')
          span.navbar-toggler-icon
        a.navbar-brand(href='#') Dashboard
        #navbarsExampleDefault.collapse.navbar-collapse
          ul.navbar-nav.mr-auto
            li.nav-item.active
              a.nav-link(href='#')
                | Home
                span.sr-only (current)
            li.nav-item
              a.nav-link(href='#') Settings
            li.nav-item
              a.nav-link(href='#') Profile
            li.nav-item
              a.nav-link(href='#') Help
          form.form-inline.mt-2.mt-md-0
            input.form-control.mr-sm-2(type='text', placeholder='Search')
            button.btn.btn-outline-success.my-2.my-sm-0(type='submit') Search




            block content
            .footer
                span.notranslate
                    p v0.1.0 Copyright 2017 Genocan, Inc. All Right Reserved
        block body-script
            script(type='text/javascript' src='/google/js/translate.js')
            //script(type='text/javascript' src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit")
